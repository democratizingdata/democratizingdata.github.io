<!doctype html>
<html id="page-None" lang="en" data-page-id="None" class="">

<head>
    <!-- Early Site Scripts. -->
    <script src="{{ site.baseurl }}/assets/js/debug.js"></script>

    <!-- Global site tag (gtag.js) & Google Analytics Code for Site.  -->
    <!-- <script async src="https://www.googletagmanager.com/gtag/js?id=G-FW7FLXLRE4"></script> -->

    <!-- Analytics Code 
    <script>
        var dntStatus = 0;
        // console.log(debugState, dntStatus);

        if (navigator.doNotTrack == "1" || window.doNotTrack == "1") {
            // console.log('debug State TRUE');
            dntStatus = 1;
        } else {
            // console.log('debug State FALSE');
            dntStatus = 0;
        }

        if (dntStatus == 1) {
            console.log('Do Not Track is enabled. Analytics disabled.');
        } else {
            window.dataLayer = window.dataLayer || [];
