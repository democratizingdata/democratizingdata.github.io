    <!-- Meta Tags. -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Performance Optimizations -->
    <link rel="preconnect" href="https://cdn.jsdelivr.net" crossorigin>
    <link rel="preconnect" href="https://code.jquery.com" crossorigin>
    <link rel="preconnect" href="https://fonts.googleapis.com" crossorigin>
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>

    <!-- Critical CSS for above-the-fold content -->
    <link rel="preload" href="{{ site.baseurl }}/assets/css/critical.css" as="style" onload="this.onload=null;this.rel='stylesheet'">
    <noscript><link rel="stylesheet" href="{{ site.baseurl }}/assets/css/critical.css"></noscript>

    <!-- Site Favicon. -->
    <link rel="shortcut icon" href="{{ site.baseurl }}/assets/img/favicon.ico">

    <!-- Site Title. -->
    <title>{{ site.title }}</title>

    <!-- All inline styles moved to _sass/utilities/performance.scss for optimal performance and caching -->

    <!-- Site Stylesheets. -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="{{ site.baseurl }}/assets/css/site.css">

    <!-- Site Scripts. -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"
        integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>

    <!-- Performance JavaScript (deferred for non-blocking load) -->
    <script src="{{ site.baseurl }}/assets/js/performance.js" defer></script>
</head>

<body>
    <!-- Site Header. -->
    {% include header.html %}

    <!-- Site Content. -->
    <main>
        {{ content }}
    </main>

    <!-- Site Footer. -->
    {% include footer.html %}

    <!-- Site Scripts. -->
    <script src="{{ site.baseurl }}/assets/js/site.js"></script>
</body>

</html>